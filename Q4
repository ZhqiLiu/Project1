head=0;
tail=0;
total=0;
i=1
while(i<=6)
    c(i)=input('input number');
    
    i=i+1;
end
i=1;
 
while(i<=6) 
while(head<c(i))
      total=total+1;
      a=rand();
      if a>=0.5
          head=head+1;
          
      else
          tail=tail+1;
          
      end
      
end
  b(i)=total;
  total=0;
  head=0;
  i=i+1;
end
  c;
  b;
bar(c,b)

